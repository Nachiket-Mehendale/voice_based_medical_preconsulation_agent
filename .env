# AI Medical Preconsultation System - Environment Variables
# Copy this file to .env and add your API keys

# =============================================================================
# REQUIRED API KEYS
# =============================================================================

# Groq API Key (Required for AI medical analysis)
# Get your key from: https://console.groq.com/
GROQ_API_KEY=your_groq_api_key_here

# =============================================================================
# OPTIONAL API KEYS
# =============================================================================

# ElevenLabs API Key (Optional - for premium text-to-speech)
# Get your key from: https://elevenlabs.io/
# Leave empty to use local TTS only
ELEVENLAB_API_KEY=

# Gemini API Key (Currently unused - placeholder for future features)
# Get your key from: https://makersuite.google.com/app/apikey
GEMINI_API_KEY=

# =============================================================================
# ELEVENLABS VOICE SETTINGS (Optional)
# =============================================================================

# Voice ID for ElevenLabs TTS
VOICE_ID=h061KGyOtpLYDxcoi8E3

# Model ID for ElevenLabs
MODEL_ID=eleven_multilingual_v2

# =============================================================================
# APPLICATION SETTINGS
# =============================================================================

# Server port for Gradio interface
SERVER_PORT=7860

# Debug mode (true/false)
DEBUG_MODE=false

# Log level (DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=INFO
